@{
    ViewBag.Title = "Inicio";
}
@if (Session["LogedUserFullname"] != null)
{
 <!-- Start content -->
    <div class="content">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <section id="cd-timeline" class="cd-container">
                        <div class="cd-timeline-block">
                            <div class="cd-timeline-img cd-success">
                                <i class="fa fa-tag"></i>
                            </div> <!-- cd-timeline-img -->

                            <div class="cd-timeline-content">
                                <h3>Cuidado Dental</h3>
                                <img src="../images/small/img1.jpg" alt="">
                                <span class="cd-date">Ofrecemos</span>
                            </div> <!-- cd-timeline-content -->
                        </div> <!-- cd-timeline-block -->

                        <div class="cd-timeline-block">
                            <div class="cd-timeline-img cd-danger">
                                <i class="fa fa-thumbs-up"></i>
                            </div> <!-- cd-timeline-img -->

                            <div class="cd-timeline-content">
                                <h3>Ortodoncia</h3>
                                <img src="../images/small/img2.jpg" alt="">
                                <span class="cd-date">Seguridad</span>
                            </div> <!-- cd-timeline-content -->
                        </div> <!-- cd-timeline-block -->

                        <div class="cd-timeline-block">
                            <div class="cd-timeline-img cd-info">
                                <i class="fa fa-star"></i>
                            </div> <!-- cd-timeline-img -->

                            <div class="cd-timeline-content">
                                <h3>Cuidado Familiar</h3>
                                <img src="../images/small/img3.jpg" alt="">
                                <span class="cd-date">Proteccion</span>
                            </div> <!-- cd-timeline-content -->
                        </div> <!-- cd-timeline-block -->
                    </section> <!-- cd-timeline -->
                </div>
            </div><!-- Row -->
        </div> <!-- content -->
    </div>
}
