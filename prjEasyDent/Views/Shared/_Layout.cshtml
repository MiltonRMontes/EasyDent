                            <ul class="list-unstyled">
                                <li><a href="@Url.Action("crearPaciente","Home")">Crear</a></li>
                                <li><a href="@Url.Action("Mandar_Paciente","Home")">Modificar</a></li>
                                <li><a href="@Url.Action("Consultar_Historia","Home")">Historia Clinica</a></li>
                            </ul>
                        </li>

                        <li>
                            <a href="#" class="waves-effect"><i class=" md md-event"></i><span> Citas </span><span class="pull-right"><i class="md md-add"></i></span></a>
